                lResult.heures = Math.floor(lDureeEnMS / (60 * 60 * 1000));
                lDureeEnMS = lDureeEnMS - lResult.heures * (60 * 60 * 1000);
                lResult.minutes = Math.floor(lDureeEnMS / (60 * 1000));
            }
            return lResult;
        }
        ;
