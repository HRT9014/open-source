                                argsIEModel: lArgsIEModel,
                                class: lParams.class
                            });
                            T.push(IE.jsx.str("div", null, lIeChips));
                        }
                    }
                }
                );
                let lResult = T.join(lParams.separateur);
                if (T.length > 0) {
                    lResult = IE.jsx.str("div", {
                        class: 'liste-url-chips'
                    }, lResult);
                }
                return lResult;
            },
