                    const lChiffreur = forge.cipher.createDecipher('AES-CBC', aCle);
                    lChiffreur.start({
                        iv: aIv
                    });
                    lChiffreur.update(aChaine);
                    return lChiffreur.finish() && lChiffreur.output.bytes();
                } catch (e) {
                    return false;
                }
            }
