            composeSpan(AParametre, AId) {
                const lID = AId || false;
                const lTabIndex = AId ? '-1' : false;
                return typeof (AParametre) === 'string' ? IE.jsx.str("span", {
                    id: lID,
                    tabindex: lTabIndex,
                    class: "sr-only"
