            estEventRetourChariot(aEventJQuery) {
                return !!aEventJQuery && aEventJQuery.which === ToucheClavier.RetourChariot && !aEventJQuery.shiftKey && !aEventJQuery.altKey && !aEventJQuery.ctrlKey;
            },
            estEventEspace(aEventJQuery) {
