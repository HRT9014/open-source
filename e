                let lMinutesEnCentiemes = Math.floor((lDuree / GDate.DureeHeures - lNbHeures) * 100);
                if (lMinutesEnCentiemes < 10) {
                    lMinutesEnCentiemes = '0' + lMinutesEnCentiemes;
                }
                return lNbHeures + ',' + lMinutesEnCentiemes;
            }
