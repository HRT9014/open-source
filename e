            construireListeUrls(aListe, aParams) {
                const lParams = Object.assign({
                    genreFiltre: null,
                    genreRessource: exports.UtilitaireUrl.genreRessourceDefault,
                    separateur: ' ',
                    IEModelChips: '',
                    argsIEModelChips: null,
                    maxWidth: 0,
                    class: ''
                }, aParams);
