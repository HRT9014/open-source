                        lOngletVisible = true;
                        return false;
                    }
                }
                );
                return lOngletVisible;
            }
            getDomainePresence(aElementRessource) {
                const lOnglet = this.getOnglet();
                if (!lOnglet.domaineDePresenceRessources) {
                    return null;
                }
