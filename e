                            };
                        }
                        if (aChaineEtape.precedent && aChaineEtape.precedent.etape === Etape.choixStrat) {
                            return await UtilitaireSecurisationCompte.suivant(aDonnees, aResultSecu, aChaineEtape.precedent);
                        }
                        break;
