                        lSurplusBaremeCalcule = (TypeNote.surplusBareme.getValeur() * aValeurBareme) / lBaremeParDefaut.getValeur();
                    } else {
                        lSurplusBaremeCalcule = TypeNote.surplusBareme.getValeur();
                    }
