                if (this._estOuvertureModale()) {
                    ObjetHtml_1.GHtml.getElement(this.nomListeConteneur).classList.remove('visible');
                    if (IE.estMobile && GNavigateur.getZIndexModalMobile) {
                        GNavigateur.getZIndexModalMobile(false);
                    }
