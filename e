                    if (!lOnglet.Actif) {
                        continue;
                    }
                    switch (lOnglet.Genre) {
                    case Enumere_Onglet_1.EGenreOnglet.FicheBrevet:
                        {
                            const lRessource = this.estEspaceAvecMembre() ? this.getMembre() : this.getUtilisateur();
