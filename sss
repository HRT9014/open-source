-- Scripts:

local function OFED_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local remote = game.ReplicatedStorage:WaitForChild("btx_remote")
	
	script.Parent.MouseButton1Click:Connect(function()
		remote:FireServer("pp_all")
		print("fired")
	end)
end
coroutine.wrap(OFED_fake_script)()
local function NZFJ_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	script.Parent.Active = true
	script.Parent.Draggable = true
end
coroutine.wrap(NZFJ_fake_script)()
